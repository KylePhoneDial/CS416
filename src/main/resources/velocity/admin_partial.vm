<table class="table table-striped">
    <thread>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Password</th>
        </tr>
    </thread>
    <tbody>
        #foreach($map in $users)
            <tr>
                <td>$map.userId</td>
                <td>$map.firstName</td>
                <td>$map.lastName</td>
                <td>$map.email</td>
                <td>$map.password</td>

                <div class="btn btn-primary col-sm-12">Update User</div>
                <div class="btn btn-warning col-sm-12">Delete User</div>
            </tr>
        #end
    </tbody>
</table>